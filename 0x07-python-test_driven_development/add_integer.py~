#!/usr/bin/python3
def add_integer(a, b=98):
    if type(a) and type(b) not in [int, float]:
        raise TypeError("a and b must be integers")
    if type(a) is float:
        a = int (a)
    if type(b) is float:
        b = int (b)
    return a + b


if __name__ == "__main__":
   print(add_integer(10, 14))
